{
  "name": "server",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file ../.env src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node24 --outfile=dist/index.js --format=esm --external:pg-native --external:canvas --external:@ngrok/ngrok --external:@ngrok/ngrok-*",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@ngrok/ngrok": "^1.4.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.212.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/instrumentation": "^0.211.0",
    "@opentelemetry/instrumentation-pino": "^0.57.0",
    "@opentelemetry/resources": "^2.5.1",
    "@opentelemetry/sdk-logs": "^0.212.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@scalar/express-api-reference": "^0.8.39",
    "@trpc/server": "^11.0.0",
    "cors": "^2.8.5",
    "express": "^5.0.1",
    "ioredis": "^5.6.1",
    "libsodium-wrappers": "^0.8.2",
    "pino": "^10.3.1",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^24.0.0",
    "esbuild": "^0.27.2",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.0.0"
  }
}
