{
  "name": "ondc-vaatun",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "d": "$npm_execpath dev:tunnel",
    "b": "$npm_execpath build",
    "build": "$npm_execpath lint && $npm_execpath format && $npm_execpath openapi:generate && bun --bun  next build",
    "lint": "biome check --write",
    "dev": "$npm_execpath openapi:watch & bun --bun next dev",
    "dev:tunnel": "$npm_execpath ngrok & $npm_execpath dev",
    "start": "bun --bun next start",
    "format": "biome format --write",
    "check": "biome check --write",
    "ngrok": "ngrok http --domain moved-starfish-rapid.ngrok-free.app 3000",
    "n": "$npm_execpath ngrok",
    "openapi:generate": "bun src/openapi/generator.ts",
    "g": "$npm_execpath openapi:generate",
    "openapi:watch": "bun --watch src/openapi/generator.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.2.0",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@scalar/nextjs-api-reference": "^0.9.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "libsodium-wrappers": "^0.7.16",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^24",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/swagger-ui-react": "^5.18.0",
    "babel-plugin-react-compiler": "1.0.0",
    "bun-types": "^1.3.5",
    "dotenv": "^17.2.3",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
